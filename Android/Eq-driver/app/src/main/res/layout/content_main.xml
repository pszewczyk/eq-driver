<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="pl.edu.pw.eiti.pszewczyk.eq_driver.MainActivity"
    tools:showIn="@layout/activity_main"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <EditText
        android:id="@+id/customRaHour"
        android:layout_width="61dp"
        android:layout_height="39dp"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintTop_toBottomOf="@+id/starSpinner"
        android:layout_marginLeft="16dp"
        app:layout_constraintLeft_toLeftOf="parent" />

    <TextView
        android:id="@+id/customLabel4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="8dp"
        android:text="'"
        android:textSize="24sp"
        app:layout_constraintLeft_toRightOf="@+id/customDecMin"
        app:layout_constraintTop_toBottomOf="@+id/starSpinner" />

    <TextView
        android:id="@+id/customLabel2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="-1dp"
        android:layout_marginTop="8dp"
        android:text="'"
        android:textSize="24sp"
        app:layout_constraintLeft_toRightOf="@+id/customRaMin"
        app:layout_constraintTop_toBottomOf="@+id/starSpinner" />

    <TextView
        android:id="@+id/customLabel3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="°"
        android:textSize="24sp"
        app:layout_constraintLeft_toRightOf="@+id/customDecDeg"
        app:layout_constraintTop_toBottomOf="@+id/starSpinner"
        android:layout_marginLeft="3dp" />

    <EditText
        android:id="@+id/customDecMin"
        android:layout_width="65dp"
        android:layout_height="39dp"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintLeft_toRightOf="@+id/customLabel3"
        app:layout_constraintTop_toBottomOf="@+id/starSpinner"
        android:layout_marginLeft="4dp" />

    <EditText
        android:id="@+id/customDecDeg"
        android:layout_width="61dp"
        android:layout_height="39dp"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintLeft_toRightOf="@+id/customRaMin"
        app:layout_constraintTop_toBottomOf="@+id/starSpinner" />

    <EditText
        android:id="@+id/customRaMin"
        android:layout_width="65dp"
        android:layout_height="39dp"
        android:layout_marginLeft="-51dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="9dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintLeft_toRightOf="@+id/customlabel1"
        app:layout_constraintTop_toBottomOf="@+id/starSpinner" />

    <TextView
        android:id="@+id/customlabel1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="h"
        android:textSize="24sp"
        app:layout_constraintLeft_toRightOf="@+id/customRaHour"
        app:layout_constraintTop_toBottomOf="@+id/starSpinner"
        android:layout_marginLeft="0dp" />

    <EditText
        android:id="@+id/GotoRAHour"
        android:layout_width="61dp"
        android:layout_height="39dp"
        android:ems="10"
        android:inputType="number"
        tools:layout_editor_absoluteX="16dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/RAPosHour" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="h"
        android:textSize="24sp"
        app:layout_constraintLeft_toRightOf="@+id/RAPosHour"
        app:layout_constraintTop_toBottomOf="@+id/RAText" />

    <EditText
        android:id="@+id/RAPosMin"
        android:layout_width="66dp"
        android:layout_height="38dp"
        android:ems="10"
        android:inputType="number"
        android:layout_marginTop="6dp"
        app:layout_constraintTop_toBottomOf="@+id/RAText"
        app:layout_constraintLeft_toRightOf="@+id/textView2"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="'"
        android:textSize="24sp"
        app:layout_constraintLeft_toRightOf="@+id/RAPosMin"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/RAText" />

    <EditText
        android:id="@+id/DECPosDeg"
        android:layout_width="61dp"
        android:layout_height="39dp"
        android:ems="10"
        android:inputType="number"
        android:layout_marginTop="7dp"
        app:layout_constraintTop_toBottomOf="@+id/DECText"
        app:layout_constraintLeft_toRightOf="@+id/textView4"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="°"
        android:textSize="24sp"
        app:layout_constraintLeft_toRightOf="@+id/DECPosDeg"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/DECText" />

    <EditText
        android:id="@+id/DECPosMin"
        android:layout_width="65dp"
        android:layout_height="39dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintLeft_toRightOf="@+id/textView7"
        android:layout_marginTop="7dp"
        app:layout_constraintTop_toBottomOf="@+id/DECText" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="'"
        android:textSize="24sp"
        app:layout_constraintLeft_toRightOf="@+id/DECPosMin"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/DECText" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="'"
        android:textSize="24sp"
        app:layout_constraintLeft_toRightOf="@+id/GotoDecMin"
        android:layout_marginTop="19dp"
        app:layout_constraintTop_toBottomOf="@+id/textView8" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="'"
        android:textSize="24sp"
        app:layout_constraintLeft_toRightOf="@+id/GotoRaMin"
        android:layout_marginTop="19dp"
        app:layout_constraintTop_toBottomOf="@+id/textView4" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="°"
        android:textSize="24sp"
        app:layout_constraintLeft_toRightOf="@+id/GotoDecDeg"
        android:layout_marginTop="19dp"
        app:layout_constraintTop_toBottomOf="@+id/textView7" />

    <EditText
        android:id="@+id/GotoDecMin"
        android:layout_width="65dp"
        android:layout_height="39dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintLeft_toRightOf="@+id/textView"
        android:layout_marginTop="9dp"
        app:layout_constraintTop_toBottomOf="@+id/DECPosMin" />

    <EditText
        android:id="@+id/GotoDecDeg"
        android:layout_width="61dp"
        android:layout_height="39dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintLeft_toRightOf="@+id/textView5"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/DECPosDeg"
        android:layout_marginStart="8dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="EQ Driver 0.1"
        android:id="@+id/textTitle"
        tools:layout_editor_absoluteY="16dp"
        tools:layout_editor_absoluteX="152dp" />

    <TextView
        android:id="@+id/RAText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="RA"
        tools:layout_editor_absoluteX="77dp"
        app:layout_constraintTop_toBottomOf="@+id/textTitle" />

    <TextView
        android:id="@+id/DECText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="DEC"
        app:layout_constraintTop_toBottomOf="@+id/textTitle"
        app:layout_constraintLeft_toRightOf="@+id/RAText"
        android:layout_marginLeft="170dp"
        android:layout_marginStart="170dp" />

    <EditText
        android:id="@+id/RAPosHour"
        android:layout_width="61dp"
        android:layout_height="39dp"
        android:ems="10"
        android:inputType="number"
        tools:layout_editor_absoluteX="14dp"
        android:layout_marginTop="6dp"
        app:layout_constraintTop_toBottomOf="@+id/RAText" />

    <EditText
        android:id="@+id/GotoRaMin"
        android:layout_width="65dp"
        android:layout_height="39dp"
        android:ems="10"
        android:inputType="number"
        app:layout_constraintLeft_toRightOf="@+id/textView3"
        android:layout_marginLeft="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintTop_toBottomOf="@+id/RAPosMin"
        android:layout_marginStart="8dp" />

    <Button
        android:id="@+id/GotoButton"
        android:layout_width="129dp"
        android:layout_height="48dp"
        android:text="GOTO"
        android:onClick="doGoto"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintHorizontal_bias="0.501"
        android:layout_marginTop="19dp"
        app:layout_constraintTop_toBottomOf="@+id/customDecMin" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="19dp"
        android:text="h"
        android:textSize="24sp"
        app:layout_constraintLeft_toRightOf="@+id/GotoRAHour"
        app:layout_constraintTop_toBottomOf="@+id/textView2" />

    <Spinner
        android:id="@+id/starSpinner"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="11dp"
        app:layout_constraintTop_toBottomOf="@+id/GotoDecMin"
        app:layout_constraintHorizontal_bias="0.0" />

    <Button
        android:id="@+id/syncButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="22dp"
        android:onClick="doSync"
        android:text="Synchronize"
        app:layout_constraintHorizontal_bias="0.501"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/GotoButton" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline"
        app:layout_constraintGuide_begin="20dp"
        android:orientation="horizontal" />

    <TextView
        android:id="@+id/statusText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:text="Initializing"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/syncButton" />

</android.support.constraint.ConstraintLayout>
